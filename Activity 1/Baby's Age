#include <stdio.h>

int main(void){
    int prog, teach, peter, baby;

    printf("Enter The Programmer's age: ");
    scanf("%d", &prog);

    printf("Enter the teacher's age: ");
    scanf("%d", &teach);

    printf("Enter Peter's age: ");
    scanf("%d", &peter);

    printf("Baby's age = %d", baby = (prog*teach/peter)+1);

    return 0;
}
